{
  "id": "dnd5e-babele-translation-files-generator",
  "title": "DnD5e - Translation files generator for Babele",
  "description": "Generate translation file for Babele by selecting a valid Compendium from dnd5e system.",
  "authors": [
    {
      "name": "DjLeChuck",
      "email": "djlechuck@gmail.com",
      "url": "https://github.com/djlechuck/",
      "flags": {}
    },
    {
      "name": "Enielka",
      "url": "https://github.com/Enielka/",
      "flags": {}
    }
  ],
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": "12",
    "verified": "12.331"
  },
  "esmodules": [
    "assets/btfg.js"
  ],
  "styles": [
    "assets/btfg.css"
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "compatibility": {
          "minimum": "4.1.0"
        }
      }
    ]
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "fr",
      "name": "Fran√ßais",
      "path": "lang/fr.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "bugs": "https://github.com/Enielka/foundryvtt-dnd5e-babele-translation-files-generator/issues",
  "changelog": "https://github.com/Enielka/foundryvtt-dnd5e-babele-translation-files-generator/blob/main/CHANGELOG.md",
  "license": "./LICENSE",
  "readme": "./README.md",
  "flags": {
    "hotReload": {
      "extensions": [
        "css",
        "hbs",
        "json"
      ],
      "paths": [
        "lang/",
        "assets/",
        "templates/"
      ]
    }
  }
}
